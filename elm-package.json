{
    "version": "0.0.1",
    "summary": "Castle of the Winds using Elm",
    "repository": "https://github.com/mordrax/cotwelm.git",
    "license": "BSD3",
    "source-directories": [
        ".",
        "src"
    ],
    "exposed-modules": [],
    "dependencies": {
        "elm-community/maybe-extra": "1.1.0 <= v < 2.0.0",
        "elm-community/random-extra": "1.0.0 <= v < 2.0.0",
        "elm-lang/core": "4.0.1 <= v < 5.0.0",
        "elm-lang/html": "1.0.0 <= v < 2.0.0",
        "elm-lang/keyboard": "1.0.0 <= v < 2.0.0",
        "elm-lang/mouse": "1.0.0 <= v < 2.0.0",
        "elm-lang/navigation": "1.0.0 <= v < 2.0.0",
        "jinjor/elm-time-travel": "1.0.9 <= v < 2.0.0"
    },
    "elm-version": "0.17.0 <= v < 0.18.0",
    "sublime-build": {
        "output": {
            "path": "dist/js/cotw.js",
            "components": {
                "ext": "js",
                "dir": "dist/js"
            }
        },
        "main": "src/main.elm"
    }
}